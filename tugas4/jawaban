1. select * from Music

2. select m.tittle,m.durasi,g.name,s.name,m.photo,m.deskripsi as "nama genre" from Music m INNER JOIN Genre g INNER JOIN Singers s ON m.id_genre=g.id  and m.id_singer=s.id WHERE id_genre=1
select m.tittle,m.durasi,g.name,s.name,m.photo,m.deskripsi as "nama genre" from Music m INNER JOIN Genre g INNER JOIN Singers s ON m.id_genre=g.id  and m.id_singer=s.id WHERE id_genre=2
select m.tittle,m.durasi,g.name,s.name,m.photo,m.deskripsi as "nama genre" from Music m INNER JOIN Genre g INNER JOIN Singers s ON m.id_genre=g.id  and m.id_singer=s.id WHERE id_genre=3
select m.tittle,m.durasi,g.name,s.name,m.photo,m.deskripsi as "nama genre" from Music m INNER JOIN Genre g INNER JOIN Singers s ON m.id_genre=g.id  and m.id_singer=s.id WHERE id_genre=4

3. select m.tittle,m.durasi,g.name,s.name,m.photo,m.deskripsi as "nama genre" from Music m INNER JOIN Genre g INNER JOIN Singers s ON m.id_genre=g.id  and m.id_singer=s.id WHERE m.id=1

